{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups":[{
    "name": "ReditAPI",
    "urls":[
      "https://reddit.com/r/pics.json",
      "https://reddit.com/r/pics.**"
    ],
    "cacheConfig": {
      "strategy": "freshness",
      "maxAge": "1d",
      "maxSize": 100,
      "timeout": "10s"
      }
    }
  ]
}
